{
  "name": "@connectedxm/admin",
  "version": "6.13.0",
  "description": "Admin API javascript SDK",
  "author": "ConnectedXM Inc.",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/connectedxm/admin-sdk.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "npm run lint && npm run build",
    "local": "npm run release && npm pack",
    "exports": "tsx scripts/exports.ts",
    "generate": "tsx scripts/generate.ts",
    "generate:sdks": "tsx scripts/generate-sdks.ts",
    "publish:sdk": "tsx scripts/publish-sdk.ts",
    "upgrade": "ncu -i --format group"
  },
  "dependencies": {
    "axios": "^1.13.5",
    "immer": "^10.1.1"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18 || ^19"
  },
  "devDependencies": {
    "tsx": "^4.21.0",
    "@babel/parser": "^7.29.0",
    "@swc/core": "^1.15.13",
    "@tanstack/react-query": "^5.90.21",
    "@types/glob": "^8.1.0",
    "@types/node": "^22.15.3",
    "@types/react": "^19.2.14",
    "@typescript-eslint/eslint-plugin": "^8.56.1",
    "@typescript-eslint/parser": "^8.56.1",
    "comment-parser": "^1.4.5",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "glob": "^11.0.2",
    "openapi-schema-validator": "^12.1.3",
    "recast": "^0.23.11",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.59.0"
  }
}
