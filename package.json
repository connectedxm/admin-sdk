{
  "name": "@connectedxm/admin",
  "version": "1.8.0-beta.9",
  "description": "Admin API javascript SDK",
  "author": "ConnectedXM Inc.",
  "repository": {
    "type": "git",
    "url": "https://github.com/connectedxm/admin-sdk.git"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vitest",
    "lint": "npm run tsc && eslint src/**/*.ts",
    "test": "vitest run",
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "release": "npm run lint && npm run test && npm run build",
    "local": "npm run release && npm pack",
    "document": "typedoc --plugin typedoc-material-theme",
    "exports": "tsx scripts/addExports.ts",
    "tsc": "tsc"
  },
  "dependencies": {
    "axios": "^1.6.5",
    "immer": "^10.0.3"
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "react": "^18.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.24.7",
    "@swc/core": "^1.3.102",
    "@tanstack/react-query": "^5.0.0",
    "@types/glob": "^8.1.0",
    "@types/node": "^20.14.2",
    "@types/react": "^18.2.47",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "glob": "^10.4.1",
    "recast": "^0.23.9",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.13",
    "typedoc-material-theme": "^1.0.2",
    "typedoc-theme-category-nav": "^0.0.3",
    "typedoc-theme-hierarchy": "^4.0.0",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.6.1"
  }
}
